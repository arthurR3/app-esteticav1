{
  "expo": {
    "name": "Estetica Emma",
    "slug": "app-esteticav1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_estetica.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arthur23.appesteticav1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_estetica.png",
        "backgroundColor": "#00000000"
      },
      "package": "com.arthur23.appesteticav1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
    "image": "./assets/images/logo_estetica.png",
    "resizeMode": "contain"
  },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f926a881-0c01-4ca3-94d9-36fe10db0cdf"
      }
    },
    "owner": "arthur23"
  }
}
